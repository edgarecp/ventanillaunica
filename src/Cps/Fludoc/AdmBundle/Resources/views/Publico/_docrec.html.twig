<table border='1' width='100%'>
   <tr>
      <td width='10%'><b>Codigo</b></td>
      {% if colilla %}
      <td width='40%'>{{ entity.id }}</td>      
      <td><b>Pin</b></td>
      <td>{{ entity.Pin }}</td>
      {% else %}
      <td colspan='3' width='40%'>{{ entity.id }}</td>
      {% endif %}
   </tr><tr>
      <td width='10%'><b>Recepcion</b></td>
      <td width='40%'>{{ entity.recepcionEl|date('d/m/Y H:i') }}</td>
      <td width='10%'><b>Cite</b></td>
      <td width='40%'>{{ entity.Cite }}</td>
   </tr><tr>
      <td><b>Referencia</b></td>
      <td colspan='3'>{{ entity.referencia }}</td>
   </tr><tr>
      <td><b>Remite</b></td>
      <td>{{ entity.remite }}</td>
      <td><b>Folio</b></td>
      <td>{{ entity.folio }} hoja(s)</td>
   </tr>
</table>
